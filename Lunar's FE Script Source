-- SERVICES --
local Players = game:GetService("Players")
local player = Players.LocalPlayer

-- GUI --
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Parent = player:WaitForChild("PlayerGui")

-- MAIN FRAME --
local MainFrame = Instance.new("Frame")
MainFrame.Size = UDim2.new(0, 400, 0, 250)
MainFrame.Position = UDim2.new(0.5, -60, 0.5, -160)
MainFrame.BackgroundColor3 = Color3.fromRGB(200, 200, 200)
MainFrame.BackgroundTransparency = 0.7
MainFrame.BorderSizePixel = 0
MainFrame.Parent = ScreenGui

Instance.new("UICorner", MainFrame).CornerRadius = UDim.new(0, 8)

-- SCROLLING FRAME (grey area) ---
local Scroll = Instance.new("ScrollingFrame") -- ✅ FIXED NAME
Scroll.Size = UDim2.new(0, 260, 0, 220)
Scroll.Position = UDim2.new(0, 0, 0, 25)
Scroll.CanvasSize = UDim2.new(0, 0, 0, 300)
Scroll.ScrollBarThickness = 6
Scroll.BackgroundColor3 = Color3.fromRGB(160, 160, 160)
Scroll.BackgroundTransparency = 0.7
Scroll.BorderSizePixel = 0
Scroll.Parent = MainFrame

Instance.new("UICorner", Scroll).CornerRadius = UDim.new(0, 8)

-- External Scroll Frame On Side (grey area) --
local Scroll1 = Instance.new("ScrollingFrame") -- ✅ FIXED NAME
Scroll1.Size = UDim2.new(0, 125, 0, 220)
Scroll1.Position = UDim2.new(0, 270, 0, 25)
Scroll1.CanvasSize = UDim2.new(0, 0, 0, 300)
Scroll1.ScrollBarThickness = 6
Scroll1.BackgroundColor3 = Color3.fromRGB(160, 160, 160)
Scroll1.BackgroundTransparency = 0.7
Scroll1.BorderSizePixel = 0
Scroll1.Parent = MainFrame

Instance.new("UICorner", Scroll1).CornerRadius = UDim.new(0, 8)

-- CLOSE BUTTON (top-right) --
local Close = Instance.new("TextButton")
Close.Size = UDim2.new(0, 22, 0, 22)
Close.AnchorPoint = Vector2.new(1, 0)
Close.Position = UDim2.new(1, 0, 0, 0)
Close.BackgroundColor3 = Color3.fromRGB(160, 160, 160)
Close.BackgroundTransparency = 0.7
Close.BorderSizePixel = 0
Close.Text = "X"
Close.TextScaled = true
Close.Font = Enum.Font.GothamBold
Close.TextColor3 = Color3.new(0, 0, 0)
Close.Parent = MainFrame

Instance.new("UICorner", Close).CornerRadius = UDim.new(0, 8)

Close.MouseButton1Click:Connect(function()
    ScreenGui:Destroy()
end)

-- Text Label (middle-top) --
local Label = Instance.new("TextLabel")
Label.Size = UDim2.new(0, 128, 0, 16)
Label.AnchorPoint = Vector2.new(1, 0)
Label.Position = UDim2.new(1, -245, 0, 0)
Label.BackgroundColor3 = Color3.fromRGB(160, 160, 160)
