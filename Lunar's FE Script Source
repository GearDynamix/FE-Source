Label.TextScaled = false
Label.TextSize = 16
Label.Font = Enum.Font.GothamBold
Label.TextColor3 = Color3.new(255, 255, 255)
Label.Parent = MainFrame

Instance.new("UICorner", Label).CornerRadius = UDim.new(0, 8)

